{
	"name": "mayeulak/breadcrumbs",
	"description": "A reusable breadcrumb component for Laravel",
	"type": "laravel-package",
	"autoload": {
		"psr-4": {
			"Bedigit\\Breadcrumbs\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Bedigit\\Breadcrumbs\\BreadcrumbServiceProvider"
			],
			"aliases": {
				"Breadcrumb": "Bedigit\\Breadcrumbs\\BreadcrumbFacade"
			}
		}
	}
}
